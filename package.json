{
	"name": "material",
	"version": "2018.8.0",
	"description": "A CSS framework based on Material Design",
	"main": "dist/material.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "./node_modules/.bin/webpack --config webpack.config.js --mode=development -w",
		"development": "npm run dev",
		"prod": "./node_modules/.bin/webpack --config webpack.config.js --mode=production -p",
		"production": "npm run prod",
		"devDocs": "cd ./docs && npm run dev",
		"prodDocs": "cd ./docs && npm run prod",
		"devAll": "npm-run-all -p devDocs dev",
		"prodAll": "npm-run-all -p prodDocs prod"
	},
	"keywords": [
		"material",
		"design",
		"css",
		"stylus"
	],
	"author": "Derkjan Super",
	"devDependencies": {
		"autoprefixer": "^9.1.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.5",
		"babel-plugin-dynamic-import-node": "^2.0.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"clean-webpack-plugin": "^0.1.19",
		"css-loader": "^1.0.0",
		"file-loader": "^1.1.11",
		"jshint": "^2.9.6",
		"mini-css-extract-plugin": "^0.4.1",
		"node-sass": "^4.9.3",
		"npm-run-all": "^4.1.3",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^5.3.0",
		"postcss-scss": "^2.0.0",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.22.1",
		"sw-precache-webpack-plugin": "^0.11.5",
		"uglifyjs-webpack-plugin": "^1.2.7",
		"webpack": "^4.16.5",
		"webpack-cli": "^3.1.0",
		"webpack-merge": "^4.1.4"
	},
	"browserslist": [
		"> 5%"
	]
}
